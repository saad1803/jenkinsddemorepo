CREATE MULTISET TABLE $ENVT_BASE.Env_Input_Param_Log , NO FALLBACK ,
     NO BEFORE JOURNAL,
     NO AFTER JOURNAL,
     CHECKSUM = DEFAULT,
     DEFAULT MERGEBLOCKRATIO
     (
      Test_Cycle_ID VARCHAR(200) CHARACTER SET LATIN CASESPECIFIC,
      Object_Type VARCHAR(200) CHARACTER SET LATIN CASESPECIFIC,
      Comparison_Type VARCHAR(200) CHARACTER SET LATIN CASESPECIFIC,
      Selection VARCHAR(200) CHARACTER SET LATIN CASESPECIFIC)
PRIMARY INDEX ( Test_Cycle_ID );
